{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowBucketLevelChanges",
            "Effect": "Allow",
            "Action": [
                "s3:PutBucketPublicAccessBlock",
                "s3:DeleteBucketPolicy",
                "s3:PutBucketAcl"
            ],
            "Resource": "arn:aws:s3:::*"
        }
    ]
}